

generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CompanyInformation {
  id          Int     @id @default(autoincrement())
  taxId       String
  tradeRegister String
  activity    String
  manager     String
  address     String
  phone       String
  fax         String?   // <- optional
  email       String
  bankName    String
}



